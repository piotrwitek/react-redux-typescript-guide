    "react-redux-typescript-scripts": "1.0.0",
    "react-testing-library": "6.1.2",
    "ts-jest": "22.4.6",
    "tslint": "5.15.0",
    "tslint-react": "4.0.0",
    "typescript": "3.1.6"
  }
