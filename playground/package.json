  "license": "MIT",
  "main": "src/index.tsx",
  "scripts": {
    "reinstall": "rm -rf node_modules && npm install",
    "prebuild": "rm -rf out",
    "build": "tsc -p ./",
    "ci-check": "npm run lint && npm run tsc && npm run test",
    "lint": "tslint -p ./",
    "tsc": "tsc -p ./ --noEmit",
    "tsc:watch": "tsc -p ./ --noEmit -w",
