    "react-redux": "5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "5.0.0-alpha.8",
    "redux": "3.7.2",
    "redux-observable": "0.17.0",
    "reselect": "3.0.1",
    "rxjs": "5.5.6",
    "tslib": "1.8.1",
    "typesafe-actions": "1.1.2",
    "utility-types": "1.0.0"
  },
  "devDependencies": {
    "@types/enzyme": "3.1.8",
