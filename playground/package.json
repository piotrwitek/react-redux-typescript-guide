    "react-router-redux": "5.0.0-alpha.8",
    "redux": "4.0.1",
    "redux-observable": "1.1.0",
    "redux-thunk": "2.3.0",
    "reselect": "4.0.0",
    "rxjs": "6.4.0",
    "rxjs-compat": "6.4.0",
    "tslib": "1.9.3",
    "typesafe-actions": "4.0.0",
    "utility-types": "3.5.0"
  },
