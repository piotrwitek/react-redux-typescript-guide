    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "jest": "22.1.4",
    "react-docgen-typescript": "1.1.0",
    "react-styleguidist": "6.1.0",
    "ts-jest": "22.0.1",
    "tslint": "5.8.0",
    "tslint-react": "3.3.3",
    "typescript": "2.6.2",
    "webpack": "3.10.0",
    "webpack-blocks": "1.0.0-rc.2"
